{
  "manifest_version": 2,

  "name": "Meet Auto Admit",
  "description": "Use this extension to auto admit google meet's invitation",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}
